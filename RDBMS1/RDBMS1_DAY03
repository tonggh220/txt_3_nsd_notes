        正则表达式
mysql> select name from  user where  name regexp  '^j' ;
Empty set (0.00 sec)

mysql> 
mysql> select name from  user where  name regexp  'y$' ;
+--------+
| name   |
+--------+
| nobody |
| chrony |
+--------+
2 rows in set (0.00 sec)

mysql> select name from  user where  name regexp  '^j|y$' ;
+--------+
| name   |
+--------+
| nobody |
| chrony |
+--------+
2 rows in set (0.00 sec)

mysql> select name from  user where  name regexp  '^a|y$' ;
+--------+
| name   |
+--------+
| adm    |
| nobody |
| chrony |
+--------+
3 rows in set (0.01 sec)

mysql> 
