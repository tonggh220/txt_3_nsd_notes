mysql> select name ,  uid  from  user where uid <= 5;
+--------+------+
| name   | uid  |
+--------+------+
| root   |    0 |
| bin    |    1 |
| daemon |    2 |
| adm    |    3 |
| lp     |    4 |
| sync   |    5 |
+--------+------+
6 rows in set (0.00 sec)

mysql> update user set  uid = uid + 1 where uid <= 5;
Query OK, 6 rows affected (0.25 sec)
Rows matched: 6  Changed: 6  Warnings: 0

mysql> select name ,  uid  from  user where uid <= 6;
+----------+------+
| name     | uid  |
+----------+------+
| root     |    1 |
| bin      |    2 |
| daemon   |    3 |
| adm      |    4 |
| lp       |    5 |
| sync     |    6 |
| shutdown |    6 |
+----------+------+
7 rows in set (0.00 sec)

mysql> 
mysql> update user set  uid = uid - 1 where uid <= 6;


mysql> select name , uid , gid from user where name = "mysql";
+-------+------+------+
| name  | uid  | gid  |
+-------+------+------+
| mysql |   27 |   27 |
+-------+------+------+
1 row in set (0.00 sec)

mysql> select name , uid , gid , (uid + gid) / 2  pjf     from user where name = "mysql"; 
+-------+------+------+---------+
| name  | uid  | gid  | pjf     |
+-------+------+------+---------+
| mysql |   27 |   27 | 27.0000 |
+-------+------+------+---------+
1 row in set (0.00 sec)

mysql> 
mysql> select name , uid , gid , (uid + gid) / 2  pjf , uid + gid  zcj   from user where name = "mysql";
+-------+------+------+---------+------+
| name  | uid  | gid  | pjf     | zcj  |
+-------+------+------+---------+------+
| mysql |   27 |   27 | 27.0000 |   54 |
+-------+------+------+---------+------+
1 row in set (0.00 sec)

